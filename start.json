{
  "daemon": true,
  "run": [
    {
      "method": "shell.run",
      "params": {
        "venv": "venv",
        "message": "python app.py",
        "on": [
          {
            "event": "/https?:\/\/[0-9.:]+/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "self.set",
      "params": {
        "session.json": {
          "url": "{{input.event[0]}}"
        }
      }
    },
    {
      "method": "browser.open",
      "params": {
        "uri": "{{self.session.url}}",
        "target": "_blank"
      }
    }
  ]
}
